    "react-copy-to-clipboard": "^5.0.3",
    "react-datetime": "^3.0.4",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "reactstrap": "^8.9.0",
    "redux": "^4.1.2",
